{
  "name": "@blockle/blocks",
  "version": "0.8.6",
  "description": "Blocks design system",
  "repository": "git@github.com:Blockle/blocks.git",
  "license": "MIT",
  "author": "Niek Saarberg <n.saarberg@gmail.com>",
  "sideEffects": [
    "./src/lib/theme/globalTheme.ts",
    "dist/side-effects/**"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./reset.css": {
      "types": "./dist/reset.css.d.ts",
      "import": "./dist/reset.css.mjs",
      "require": "./dist/reset.css.cjs"
    },
    "./themes/momotaro": {
      "types": "./dist/themes/momotaro.d.ts",
      "import": "./dist/themes/momotaro.mjs",
      "require": "./dist/themes/momotaro.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "reset.css",
    "themes/momotaro"
  ],
  "scripts": {
    "build": "crackle package && node ./temp-use-client-fix.js",
    "build-storybook": "storybook build",
    "coverage": "vitest run --coverage",
    "fix": "crackle fix",
    "lint": "eslint .",
    "storybook": "storybook dev -p 6006 --no-open",
    "test": "vitest",
    "ts": "tsc --noemit --project ./tsconfig.json",
    "release": "yarn ts && yarn test && yarn build && changeset publish"
  },
  "resolutions": {
    "string-width": "^4.2.2"
  },
  "dependencies": {
    "@vanilla-extract/css": "^1.13.0",
    "@vanilla-extract/css-utils": "^0.1.3",
    "@vanilla-extract/sprinkles": "^1.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@crackle/cli": "^0.12.4",
    "@crackle/core": "^0.28.0",
    "@storybook/addon-a11y": "^7.5.3",
    "@storybook/addon-coverage": "^0.0.9",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/blocks": "^7.5.3",
    "@storybook/jest": "^0.2.3",
    "@storybook/react": "^7.5.3",
    "@storybook/react-vite": "^7.5.3",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.0",
    "@types/react": "^18.2.34",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vanilla-extract/vite-plugin": "^3.8.2",
    "@vitest/coverage-v8": "^0.34.6",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-unicorn": "^49.0.0",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.3",
    "prop-types": "^15.8.1",
    "storybook": "^7.5.3",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "packageManager": "yarn@1.22.19"
}
