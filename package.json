{
  "name": "@blockle/blocks",
  "version": "0.11.1",
  "description": "Blocks design system",
  "repository": "git@github.com:Blockle/blocks.git",
  "license": "MIT",
  "author": "Niek Saarberg <n.saarberg@gmail.com>",
  "sideEffects": [
    "./src/lib/theme/globalTheme.ts",
    "dist/side-effects/**"
  ],
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.mts",
        "require": "./dist/index.d.ts"
      },
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./reset.css": {
      "types": {
        "import": "./dist/reset.css.d.mts",
        "require": "./dist/reset.css.d.ts"
      },
      "import": "./dist/reset.css.mjs",
      "require": "./dist/reset.css.cjs"
    },
    "./themes/momotaro": {
      "types": {
        "import": "./dist/themes/momotaro.d.mts",
        "require": "./dist/themes/momotaro.d.ts"
      },
      "import": "./dist/themes/momotaro.mjs",
      "require": "./dist/themes/momotaro.cjs"
    },
    "./dist/style.css": "./dist/style.css",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "reset.css",
    "themes/momotaro"
  ],
  "scripts": {
    "build": "crackle package && node ./temp-use-client-fix.js",
    "build-storybook": "storybook build",
    "coverage": "vitest run --coverage",
    "fix": "crackle fix",
    "lint": "eslint .",
    "release": "yarn ts && yarn test && yarn build && changeset publish",
    "storybook": "storybook dev -p 6006 --no-open",
    "test": "vitest",
    "ts": "tsc --noemit --project ./tsconfig.json"
  },
  "resolutions": {
    "string-width": "^4.2.2"
  },
  "dependencies": {
    "@storybook/test": "^8.0.2",
    "@vanilla-extract/css": "^1.14.1",
    "@vanilla-extract/css-utils": "^0.1.3",
    "@vanilla-extract/sprinkles": "^1.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@crackle/cli": "^0.15.2",
    "@crackle/core": "^0.33.1",
    "@storybook/addon-a11y": "^8.0.2",
    "@storybook/addon-coverage": "^1.0.1",
    "@storybook/addon-essentials": "^8.0.2",
    "@storybook/addon-interactions": "^8.0.2",
    "@storybook/addon-links": "^8.0.2",
    "@storybook/addons": "^7.6.17",
    "@storybook/blocks": "^8.0.2",
    "@storybook/react": "^8.0.2",
    "@storybook/react-vite": "^8.0.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.2",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "@vanilla-extract/vite-plugin": "^4.0.6",
    "@vitest/coverage-v8": "^1.4.0",
    "autoprefixer": "^10.4.18",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-unicorn": "^51.0.1",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "prop-types": "^15.8.1",
    "storybook": "^8.0.2",
    "typescript": "^5.4.2",
    "vitest": "^1.4.0"
  },
  "packageManager": "yarn@1.22.19"
}
