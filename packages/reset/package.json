{
  "name": "@blockle/blocks-reset",
  "version": "0.24.1",
  "description": "CSS reset for Blockle",
  "type": "module",
  "sideEffects": [
    "**/reset.css.*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "vite build",
    "test": "vitest ",
    "lint": "biome format ./src",
    "format": "biome format --write ./src",
    "ts": "tsc --noemit --project ./tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Blockle/blocks.git"
  },
  "author": "Niek Saarberg <n.saarberg@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Blockle/blocks/issues"
  },
  "homepage": "https://github.com/Blockle/blocks#readme",
  "dependencies": {
    "@vanilla-extract/css": "^1.17.5"
  },
  "peerDependencies": {
    "@blockle/blocks-core": ">=1.2.x"
  }
}
